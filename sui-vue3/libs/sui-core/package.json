{
  "name": "@siteminder/sui-core",
  "version": "23.4.0-vue3",
  "type": "module",
  "description": "",
  "main": "sui-core.mjs",
  "types": "sui-core.d.ts",
  "exports": {
    ".": {
      "types": "./sui-core.d.ts",
      "import": "./sui-core.mjs",
      "require": "./sui-core.mjs"
    },
    "./components/*": {
      "types": "./components/*.d.ts",
      "import": "./components/*.mjs",
      "require": "./components/*.mjs"
    },
    "./libs": {
      "types": "./libs.d.ts",
      "import": "./libs.mjs",
      "require": "./libs.mjs"
    },
    "./services": {
      "types": "./services.d.ts",
      "import": "./services.mjs",
      "require": "./services.mjs"
    },
    "./sui-core.esm.css": "./sui-core.esm.css",
    "./sui-global.css": "./sui-global.css",
    "./sui-common.css": "./sui-common.css",
    "./sui-resets.css": "./sui-resets.css",
    "./sui-scaffolding.css": "./sui-scaffolding.css",
    "./sui-typography.css": "./sui-typography.css",
    "./sui-utilities.css": "./sui-utilities.css"
  },
  "scripts": {
    "audit": "npm audit --omit=dev",
    "build-only": "rm -rf dist/ && npm run build-only:sui-core",
    "build-only:sui-core": "vite build",
    "bundle-check": "bundlesize",
    "bundle-report": "rollup-plugin-visualizer reports/bundle-report.json --filename reports/bundle-report.html",
    "clean": "../../node_modules/@siteminder/overlord/shared/project/clean.sh",
    "compile": "npm run translations && npm run build-only",
    "esplint": "./node_modules/.bin/esplint",
    "extra-checks": "npm run clean && npm run compile && vue-tsc --project tsconfig.vue-tsc.json --noEmit && npm run bundle-check",
    "jest": "TZ=UTC jest --coverage",
    "jest-fast": "TZ=UTC jest --watch --maxWorkers=25%",
    "link:sui-core": "cp package* dist/ && cd dist && npm link",
    "lint:css:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "lint:css": "npx stylelint 'src/**/*.{vue,scss}' --quiet",
    "lint:eslint:fix": "eslint ./src/ --ext .ts,.vue --fix",
    "lint:eslint": "eslint ./src/ --ext .ts,.vue --quiet",
    "lint:fix": "npm run lint:eslint:fix && npm run lint:css:fix",
    "lint": "npm run translations && npm run lint:eslint && npm run lint:css && npm run esplint",
    "smpublish": "../../node_modules/@siteminder/overlord/shared/project/publish.sh",
    "test": "npm run translations && npm run jest",
    "test:single": "TZ=UTC jest --coverage --config --watch --maxWorkers=25%",
    "translations": "sm-translate.js --system=sui --component=sui-core --src src/app --dist src/app/lang"
  },
  "peerDependencies": {
    "vue": "~3.4.38",
    "vue-i18n": "^8 || ^9",
    "vue-router": "^3 || ^4"
  },
  "dependencies": {
    "@ckpack/vue-color": "1.5.0",
    "@popperjs/core": "2.11.8",
    "@vee-validate/i18n": "4.11.3",
    "@vee-validate/rules": "4.11.3",
    "@vueuse/core": "10.2.1",
    "fecha": "4.2.3",
    "floating-vue": "5.2.2",
    "lodash-es": "4.17.21",
    "v-calendar": "3.1.2",
    "vee-validate": "4.11.3",
    "vue-multiselect": "3.0.0-beta.3",
    "vuedraggable": "4.1.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.23.9",
    "@rollup/plugin-terser": "^0.4.4",
    "@siteminder/babel-config": "^0.4.0",
    "@siteminder/eslint-config": "^5.1.0",
    "@siteminder/sm-translate": "2.0.0",
    "@siteminder/sui-icons": "23.4.0-vue3",
    "@stylistic/eslint-plugin": "^1.8.1",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "8.1.0",
    "@types/jest": "^29.5.13",
    "@types/lodash-es": "4.17.12",
    "@types/node": "^18.16.18",
    "@types/resize-observer-browser": "^0.1.7",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@vitejs/plugin-vue": "5.0.3",
    "@vue/compiler-sfc": "3.4.38",
    "@vue/test-utils": "2.4.6",
    "@vue/tsconfig": "^0.4.0",
    "@vue/vue3-jest": "^29.2.6",
    "bundlesize": "^0.18.1",
    "chance": "^1.1.11",
    "eslint": "^8.56.0",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "esplint": "^0.10.1",
    "flush-promises": "^1.0.2",
    "glob": "^10.3.3",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "rollup-plugin-copy": "^3.5.0",
    "rollup-plugin-typescript2": "0.34.1",
    "rollup-plugin-visualizer": "^5.9.2",
    "sass": "^1.63.6",
    "stylelint": "^15.10.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^33.0.0",
    "stylelint-config-standard-scss": "^10.0.0",
    "stylelint-config-standard-vue": "^1.0.0",
    "ts-jest": "^29.2.5",
    "tslib": "2.5.3",
    "typescript": "5.0.4",
    "vite": "5.0.11",
    "vite-plugin-dts": "3.7.0",
    "vite-plugin-lib-inject-css": "^2.1.1",
    "vue": "3.4.38",
    "vue-i18n": "9.9.1",
    "vue-router": "4.2.5",
    "vue-tsc": "1.8.27",
    "wicg-inert": "^3.1.2"
  },
  "engines": {
    "node": ">14.0.0",
    "npm": ">=6.0.0"
  },
  "bundlesize": [
    {
      "path": "./dist/sui-core.mjs",
      "maxSize": "5 kB"
    },
    {
      "path": "./dist/**/sm-calendar*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./dist/**/sm-color-picker*.js",
      "maxSize": "12 kB"
    },
    {
      "path": "./dist/**/sm-date-picker*.js",
      "maxSize": "45 kB"
    },
    {
      "path": "./dist/**/sm-multi-select*.js",
      "maxSize": "8 kB"
    },
    {
      "path": "./dist/**/sm-select*.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./dist/**/sm-tooltip*.js",
      "maxSize": "20 kB"
    },
    {
      "path": "./dist/**/vue-i18n*.js",
      "maxSize": "15 kB"
    },
    {
      "path": "./dist/**/vuedraggable*.js",
      "maxSize": "40 kB"
    },
    {
      "path": "./dist/**/!(sm-calendar|sm-color-picker|sm-date-picker|sm-multi-select|sm-select|sm-tooltip|vue-i18n|vuedraggable|sm-time-picker)*.js",
      "maxSize": "5 kB"
    }
  ],
  "overrides": {
    "@siteminder/sui-icons": {
      "vue": "$vue"
    }
  }
}
