{
  "name": "goldeneye",
  "version": "23.4.0-vue3",
  "description": "SUI sample project - CampMinder (Vue3)",
  "author": "",
  "private": true,
  "scripts": {
    "audit": "npm audit --omit=dev",
    "bundle-report": "npm_config_report=true VUE_COMPAT_MODE=3 webpack --config node_modules/@siteminder/webpack-config/lib/prod.js",
    "clean": "../../node_modules/@siteminder/overlord/shared/project/clean.sh",
    "compile": "npm run translations && VUE_COMPAT_MODE=3 webpack --config node_modules/@siteminder/webpack-config/lib/prod.js",
    "dev": "VUE_COMPAT_MODE=3 webpack serve --progress --config ./webpack/dev.js",
    "esplint": "./node_modules/.bin/esplint",
    "esplint:overwrite": "./node_modules/.bin/esplint --overwrite",
    "graphql-codegen:all": "run-p 'graphql-codegen:!(all)'",
    "graphql-codegen:goldeneye": "graphql-codegen -c codegen/goldeneye.yml",
    "jest": "npm run jest:unit",
    "jest-fast:unit": "./node_modules/.bin/jest --config jest.config.cjs --watch",
    "jest:coverage": "npm run jest:unit -- --coverage",
    "jest:unit": "TZ=UTC node --max-old-space-size=4096 ./node_modules/.bin/jest --config jest.config.cjs --coverage",
    "lint": "npm run translations && npx eslint src/ --ext .ts,.vue --quiet && npm run lint:css && npm run esplint && npm run vue-tsc",
    "lint:css": "npx stylelint 'src/**/*.{vue,scss}'",
    "lint:css:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "lint:fix": "npx eslint src/ --ext .ts,.vue --fix && npm run lint:css:fix",
    "package": "npm run compile && ../../node_modules/@siteminder/overlord/shared/project/package-frontend.sh",
    "test": "npm run jest",
    "translations": "sm-translate.js",
    "vue-tsc": "vue-tsc --project tsconfig.json --noEmit"
  },
  "dependencies": {
    "@apollo/client": "^3.11.8",
    "@js-joda/core": "^5.5.3",
    "@siteminder/sui-core": "23.4.0-vue3",
    "@siteminder/sui-icons": "23.4.0-vue3",
    "@siteminder/sui-themes": "23.4.0-vue3",
    "@vee-validate/i18n": "4.11.3",
    "@vue/apollo-composable": "4.0.0-beta.8",
    "@vue/compat": "3.4.38",
    "@vueuse/core": "^10.2.1",
    "graphql": "^16.7.1",
    "normalize.css": "^8.0.1",
    "unfetch": "4.2.0",
    "vee-validate": "4.11.3",
    "vue": "3.4.38",
    "vue-i18n": "9.14.2",
    "vue-router": "4.2.5"
  },
  "devDependencies": {
    "@graphql-codegen/add": "^5.0.3",
    "@graphql-codegen/cli": "^5.0.3",
    "@graphql-codegen/typescript": "^4.1.3",
    "@graphql-codegen/typescript-operations": "^4.4.1",
    "@siteminder/babel-config": "^0.3.0",
    "@siteminder/base-typescript": "^0.4.0",
    "@siteminder/eslint-config": "^5.1.0",
    "@siteminder/sm-translate": "^2.0.0",
    "@siteminder/testing": "^5.0.0",
    "@siteminder/webpack-config": "^10.0.0",
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/user-event": "^14.5.2",
    "@testing-library/vue": "8.1.0",
    "@types/jest": "^29.5.13",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.16.19",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vue/compiler-sfc": "3.4.38",
    "@vue/runtime-dom": "3.4.38",
    "@vue/vue3-jest": "29.2.6",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^5.4.0",
    "eslint-plugin-testing-library": "^6.3.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "esplint": "^0.10.1",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-preview": "^0.3.1",
    "msw": "^1.2.2",
    "npm-run-all": "4.1.5",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "resize-observer-polyfill": "^1.5.1",
    "stylelint": "^15.10.1",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-scss": "^5.0.1",
    "ts-jest": "^29.2.5",
    "type-fest": "^3.13.0",
    "typescript": "5.1.6",
    "vue-tsc": "1.8.27"
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  }
}
