{
  "name": "docs",
  "version": "23.4.0-vue3",
  "description": "",
  "scripts": {
    "audit": "npm audit --omit=dev",
    "clean": "../../node_modules/@siteminder/overlord/shared/project/clean.sh",
    "compile": "npm run compile-icons && npm run compile-core && npm run compile-theming && npm run link-libs && NODE_OPTIONS=\"--openssl-legacy-provider --max-old-space-size=4096\" build-storybook -o ./build",
    "compile-core": "cd ../../libs/sui-core && ../../node_modules/@siteminder/overlord/shared/project/bootstrap.sh && npm run compile && cd -",
    "compile-icons": "cd ../../libs/sui-icons && ../../node_modules/@siteminder/overlord/shared/project/bootstrap.sh && npm run compile && cd -",
    "compile-theming": "cd ../../libs/sui-themes && ../../node_modules/@siteminder/overlord/shared/project/bootstrap.sh && npm run compile && cd -",
    "dev": "npm run link-libs && NODE_OPTIONS=--openssl-legacy-provider start-storybook -p 6006 --ci",
    "link-libs": "rm -rf libs && ln -s ../../libs .",
    "lint": "npm run lint:jsx && npm run lint:mdx",
    "lint:fix": "npm run lint:jsx:fix && npm run lint:mdx:fix",
    "lint:jsx": "eslint . --ext .jsx",
    "lint:jsx:fix": "eslint . --fix --ext .jsx",
    "lint:mdx": "eslint . -c eslintrc-mdx.json --ext .mdx --no-eslintrc",
    "lint:mdx:fix": "eslint . --fix -c eslintrc-mdx.json --ext .mdx --no-eslintrc",
    "package": "npm run compile && ../../node_modules/@siteminder/overlord/shared/project/package-frontend.sh",
    "percy:dev": "./run-percy-warning.sh && percy storybook https://sui-dev-sui-docs.dev.siteminderlabs.com/",
    "percy:local": "./run-percy-warning.sh && percy storybook http://localhost:6006/",
    "percy:local-build": "./run-percy-warning.sh && npm run compile && percy storybook ./build",
    "percy:prod": "./run-percy-warning.sh && percy storybook https://sui.siteminder.systems/",
    "percy:vue3": "./run-percy-warning.sh && percy storybook https://sui-dev-v3-sui-docs.dev.siteminderlabs.com/",
    "test": "exit 0"
  },
  "private": true,
  "type": "module",
  "license": "UNLICENSED",
  "devDependencies": {
    "@babel/core": "^7.20.2",
    "@percy/cli": "1.31.1",
    "@percy/storybook": "6.0.3",
    "@storybook/addon-a11y": "6.5.16",
    "@storybook/addon-actions": "6.5.16",
    "@storybook/addon-docs": "6.5.16",
    "@storybook/addon-essentials": "6.5.16",
    "@storybook/addon-interactions": "6.5.16",
    "@storybook/addon-links": "6.5.16",
    "@storybook/builder-vite": "0.2.7",
    "@storybook/testing-library": "0.2.2",
    "@storybook/theming": "6.5.16",
    "@storybook/vue3": "6.5.16",
    "@vitejs/plugin-vue": "3.2.0",
    "autoprefixer": "^10.4.13",
    "babel-loader": "^8.3.0",
    "eslint": "^8.36.0",
    "eslint-plugin-mdx": "^2.0.5",
    "eslint-plugin-react": "^7.32.2",
    "sass": "^1.56.1",
    "storybook-addon-designs": "^6.3.1",
    "storybook-vue3-router": "2.3.1",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.5",
    "vite": "3.2.7",
    "vue-loader": "17.4.2",
    "vue-tsc": "1.8.27"
  },
  "dependencies": {
    "vue": "3.4.38",
    "vue-i18n": "9.14.2"
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  }
}
