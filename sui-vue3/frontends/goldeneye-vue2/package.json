{
  "name": "goldeneye-vue2",
  "version": "9.1.0",
  "description": "SUI sample project - CampMinder (Vue2)",
  "author": "",
  "private": true,
  "scripts": {
    "audit": "npm audit --omit=dev",
    "bundle-report": "npm_config_report=true webpack --config node_modules/@siteminder/webpack-config/lib/prod.js",
    "clean": "../../node_modules/@siteminder/overlord/shared/project/clean.sh",
    "compile": "npm run translations && webpack --config node_modules/@siteminder/webpack-config/lib/prod.js",
    "dev": "webpack serve --progress --config node_modules/@siteminder/webpack-config/lib/dev.js",
    "esplint": "./node_modules/.bin/esplint",
    "jest": "npm run jest:unit",
    "jest-fast:unit": "./node_modules/.bin/jest --config jest.config.js --watch",
    "jest:coverage": "npm run jest:unit -- --coverage",
    "jest:unit": "TZ=UTC node --max-old-space-size=4096 ./node_modules/.bin/jest --config jest.config.js --passWithNoTests --coverage",
    "lint": "npm run translations && npx eslint src/ --ext .ts,.vue --quiet && npm run lint:css && npm run esplint",
    "lint:css": "npx stylelint 'src/**/*.{vue,scss}'",
    "lint:css:fix": "npx stylelint 'src/**/*.{vue,scss}' --fix",
    "lint:fix": "npx eslint src/ --ext .ts,.vue --fix && npm run lint:css:fix",
    "package": "npm run compile && ../../node_modules/@siteminder/overlord/shared/project/package-frontend.sh",
    "test": "npm run jest",
    "translations": "sm-translate.js",
    "vue-tsc": "vue-tsc --project tsconfig.volar.json --noEmit"
  },
  "dependencies": {
    "@js-joda/core": "^5.5.3",
    "@siteminder/sui-core": "9.1.0",
    "@siteminder/sui-icons": "10.1.0",
    "@siteminder/sui-themes": "9.1.0",
    "@vue/apollo-composable": "4.0.0-beta.8",
    "@vueuse/core": "^10.2.1",
    "graphql": "^16.7.1",
    "normalize.css": "^8.0.1",
    "vee-validate": "^3.4.14",
    "vue": "2.7.16",
    "vue-i18n": "^8.28.2",
    "vue-router": "3.6.5"
  },
  "devDependencies": {
    "@siteminder/babel-config": "^0.3.0",
    "@siteminder/base-typescript": "^0.4.0",
    "@siteminder/eslint-config": "^4.0.0",
    "@siteminder/sm-translate": "^2.0.0",
    "@siteminder/testing": "^5.0.0",
    "@siteminder/webpack-config": "^8.0.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^14.4.3",
    "@testing-library/vue": "^5.8.3",
    "@types/jest": "^29.5.3",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.16.19",
    "@typescript-eslint/eslint-plugin": "^5.62.0",
    "@typescript-eslint/parser": "^5.62.0",
    "@vue/test-utils": "^1.3.3",
    "@vue/vue2-jest": "^29.2.2",
    "eslint": "^8.44.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-import-resolver-typescript": "^3.5.5",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-jest-dom": "^5.0.1",
    "eslint-plugin-testing-library": "5.11.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.9.0",
    "esplint": "^0.10.1",
    "jest": "^29.6.1",
    "jest-environment-jsdom": "^29.6.1",
    "jest-jasmine2": "^29.6.1",
    "jest-preview": "^0.3.1",
    "msw": "^1.2.2",
    "postcss-html": "^1.5.0",
    "prettier": "^2.8.8",
    "stylelint": "^15.10.1",
    "stylelint-config-recess-order": "^4.2.0",
    "stylelint-config-recommended-scss": "^12.0.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-scss": "^5.0.1",
    "ts-jest": "^29.0.5",
    "type-fest": "^3.13.0",
    "typescript": "5.1.6",
    "vue-template-compiler": "2.7.16",
    "vue-tsc": "1.8.4"
  },
  "engines": {
    "node": ">=18.17.1",
    "npm": ">=9.6.7"
  }
}
